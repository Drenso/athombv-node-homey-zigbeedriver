  "author": "Athom B.V.",
  "license": "ISC",
  "dependencies": {
    "color-space": "^1.16.0",
    "zigbee-clusters": "git+ssh://git@github.com/athombv/node-zigbee-clusters.git#production"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-athom": "^2.0.8",
    "grunt": "^1.0.4",
    "grunt-contrib-connect": "^2.1.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-jsdoc": "^2.4.1",
    "homey": "^2.1.5"
  },
  "engines": {
    "node": ">=12"
  }
}
