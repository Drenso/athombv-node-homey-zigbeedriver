{
  "name": "homey-zigbeedriver",
  "version": "1.0.0",
  "description": "This module can be used to make the development of Zigbee apps for Homey easier.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "build": "grunt jsdoc",
    "serve": "grunt connect",
    "build:watch": "npm run build && concurrently -n serve,watch 'npm run serve' 'grunt watch'"
  },
  "keywords": [
    "zigbee",
    "homey"
